				<xs:sequence>
					<xs:element name="PostalOrderEvent" type="ns1:PostalOrderEvent" minOccurs="0" maxOccurs="unbounded"></xs:element>
				</xs:sequence>
			</xs:complexType>
			<xs:element name="PostalOrderEventsForMailInput" type="ns1:PostalOrderEventsForMailInput"></xs:element>
			<xs:complexType name="PostalOrderEventsForMailInput">
				<xs:sequence></xs:sequence>
				<xs:attribute name="Barcode" use="required" type="ns1:Barcode"></xs:attribute>
				<xs:attribute name="Language" type="xs:string" default="RUS"></xs:attribute>
			</xs:complexType>
			<xs:simpleType name="Barcode">
				<xs:union memberTypes="ns1:BarcodeS10 ns1:BarcodeRTM1"></xs:union>
			</xs:simpleType>
			<xs:simpleType name="BarcodeRTM1">
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-9]{14}"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
			<xs:element name="PostalOrderEventsForMailFault" type="ns1:PostalOrderEventsForMailFault"></xs:element>
			<xs:complexType name="PostalOrderEventsForMailFault">
				<xs:sequence>
