				</xs:sequence>
			</xs:complexType>
			<xs:element name="PostalOrderEventsForMailError" type="ns1:PostalOrderEventsForMailError"></xs:element>
			<xs:complexType name="PostalOrderEventsForMailError">
				<xs:sequence></xs:sequence>
				<xs:attribute name="ErrorID" use="optional" type="ns1:PostalOrderEventsForMailErrors"></xs:attribute>
			</xs:complexType>
			<xs:simpleType name="PostalOrderEventsForMailErrors">
				<xs:restriction base="xs:string">
					<xs:enumeration value="InvalidLoginOrPassword"></xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:element name="PostalOrderEvent" type="ns1:PostalOrderEvent"></xs:element>
			<xs:complexType name="PostalOrderEvent">
				<xs:sequence></xs:sequence>
				<xs:attribute name="Number" use="optional" type="ns1:NumberPostalOrder"></xs:attribute>
				<xs:attribute name="EventDateTime" use="optional" type="xs:dateTime"></xs:attribute>
				<xs:attribute name="EventType" use="optional" type="xs:integer"></xs:attribute>
				<xs:attribute name="EventName" use="optional" type="xs:string"></xs:attribute>
				<xs:attribute name="IndexTo" use="optional" type="xs:integer"></xs:attribute>
				<xs:attribute name="IndexEvent" use="optional" type="xs:integer"></xs:attribute>
				<xs:attribute name="SumPaymentForward" use="optional" type="xs:integer"></xs:attribute>
				<xs:attribute name="CountryEventCode" type="xs:string" use="optional"></xs:attribute>
				<xs:attribute name="CountryToCode" type="xs:string" use="optional"></xs:attribute>
			</xs:complexType>
			<xs:simpleType name="NumberPostalOrder">
				<xs:restriction base="xs:string"></xs:restriction>
			</xs:simpleType>
			<xs:element name="Login" type="xs:string"></xs:element>
			<xs:element name="Password" type="xs:string"></xs:element>
		</xs:schema>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://russianpost.org/operationhistory/data" xmlns:env="http://schemas.xmlsoap.org/soap/envelope/" targetNamespace="http://russianpost.org/operationhistory/data" elementFormDefault="qualified">
			<xsd:element name="AuthorizationHeader">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="login" type="xsd:string"></xsd:element>
						<xsd:element name="password" type="xsd:string"></xsd:element>
					</xsd:sequence>
					<xsd:attribute ref="env:mustUnderstand" default="1"></xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="UpdateOperationRequest" type="tns:UpdateOperationRequest"></xsd:element>
			<xsd:complexType name="UpdateOperationRequest">
				<xsd:sequence>
					<xsd:element name="RequestType" type="tns:RequestType" minOccurs="1" maxOccurs="1"></xsd:element>
					<xsd:element name="SourceOperation" type="tns:OperationHistoryRecord" minOccurs="1" maxOccurs="1"></xsd:element>
					<xsd:element name="TargetOperation" type="tns:OperationHistoryRecord" minOccurs="0" maxOccurs="1"></xsd:element>
					<xsd:element name="ReasonDescription" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
					<xsd:element name="InitiatorDepartment" type="xsd:integer" minOccurs="1" maxOccurs="1"></xsd:element>
					<xsd:element name="ExecutorIP" type="xsd:string" minOccurs="1" maxOccurs="1"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:simpleType name="Barcode">
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[0-9]{14}|[A-Z]{2}[0-9]{9}[A-Z]{2}"></xsd:pattern>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:element name="OperationHistoryData">
